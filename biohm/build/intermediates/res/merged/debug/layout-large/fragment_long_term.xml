<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.shemanigans.mime.activities.OverviewActivity$PlaceholderFragment" >

    <TextView
        android:id="@+id/device_name_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/label_device_name"
        android:textIsSelectable="true"
        android:textSize="20sp" />

    <Space
        android:id="@+id/device_name_space_tag"
        android:layout_width="5dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/device_address_tag" />

    <TextView
        android:id="@+id/device_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/device_address_space_tag"
        android:textIsSelectable="true"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/device_address_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/device_name_tag"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:text="@string/label_device_address"
        android:textIsSelectable="true"
        android:textSize="20sp" />

    <Space
        android:id="@+id/device_address_space_tag"
        android:layout_width="5dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/device_name_tag"
        android:layout_toRightOf="@+id/device_address_tag"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin" />

    <TextView
        android:id="@+id/device_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/device_name_tag"
        android:layout_toRightOf="@+id/device_address_space_tag"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:textIsSelectable="true"
        android:textSize="20sp" />

    <com.androidplot.xy.XYPlot
        android:id="@+id/bioimpedancePlot"
        android:layout_width="330dp"
        android:layout_height="200dp"
        android:layout_below="@+id/device_address"
        android:layout_gravity="center_horizontal"
        title="Bioimpedance Data"
        android:visibility="visible" />

    <TextView
        android:id="@+id/parameter_summary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bioimpedancePlot"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:text="@string/parameter_summary"
        android:textColor="@color/primary"
        android:textSize="22sp"
        android:textStyle="bold" />

    <TableLayout
        android:id="@+id/summary_table"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/parameter_summary"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/activity_vertical_margin" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/sample_rate" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/start_freq" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/step_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/num_of_increments" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/sample_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/not_applicable" />

            <TextView
                android:id="@+id/start_freq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/not_applicable" />

            <TextView
                android:id="@+id/step_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/not_applicable" />

            <TextView
                android:id="@+id/num_of_increments"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:padding="3dip"
                android:text="@string/not_applicable" />
        </TableRow>
    </TableLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true" >

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="4dip"
            android:layout_marginRight="4dip"
            android:background="?android:attr/dividerVertical" />

        <View
            android:id="@+id/ViewColorPickerHelper"
            android:layout_width="1dip"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="4dip"
            android:layout_marginTop="4dip"
            android:background="?android:attr/dividerVertical" />

        <Button
            android:id="@+id/export_to_text_frag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/ViewColorPickerHelper"
            android:background="?android:attr/selectableItemBackground"
            android:text="@string/exportToText"
            android:textColor="@color/primary"
            android:textSize="24sp"
            android:textStyle="bold|italic" />

        <Button
            android:id="@+id/clear_text_file_frag"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/ViewColorPickerHelper"
            android:background="?android:attr/selectableItemBackground"
            android:text="@string/clearTextFile"
            android:textColor="@color/primary"
            android:textSize="24sp"
            android:textStyle="bold|italic" />
    </RelativeLayout>

</RelativeLayout><!-- From: file:/Users/Shemanigans/AndroidstudioProjects/Biohm/biohm/src/main/res/layout-large/fragment_long_term.xml -->